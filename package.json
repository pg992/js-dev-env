{
  "name": "building-js-dev-env",
  "version": "1.0.0",
  "description": "Building javascript dev enviroment",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch",
    "open:src": "babel-node buildScripts/srcServer.js",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pg992/js-dev-env.git"
  },
  "author": "Petar Gjeorgiev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pg992/js-dev-env/issues"
  },
  "homepage": "https://github.com/pg992/js-dev-env#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chalk": "^2.3.0",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^5.1.2",
    "eslint": "^4.13.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-watch": "^3.1.3",
    "express": "^4.16.2",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.5.1",
    "localtunnel": "^1.8.3",
    "mocha": "^4.0.1",
    "mock": "^0.1.1",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.1.0",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.2",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-md5-hash": "0.0.6"
  }
}
